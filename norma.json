{
  "name": "junction-framework",
  "tasks": {
    "sass": {
      "src": "lib/**/*",
      "dest": "out/",
      "comb": true,
      "lint": false,
      "minify": true
    },
    "copy": {
      "src": "out/framework",
      "dest": "./",
      "order": "post",
      "ext": [
        "css"
      ]
    },
    "dss": {
      "src": "lib/**/*",
      "dest": "http://localhost:3000/api/v1/blocks"
    }
  }
}
